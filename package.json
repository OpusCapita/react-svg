{
  "name": "@opuscapita/react-svg",
  "version": "3.0.2",
  "description": "React components for SVG (Scalable Vector Graphic).",
  "scripts": {
    "npm-build": "rimraf lib && webpack --config webpack.production.config.js",
    "link-mode": "webpack --watch --config ./webpack.link.config.js",
    "lint": "eslint src/client",
    "start": "nodemon --watch www www/index --exec babel-node --presets es2015,stage-0,react --plugins transform-decorators-legacy",
    "npm-publish": "npm run npm-build && npm-publish",
    "publish-snapshot": "npm run npm-publish",
    "publish-release": "npm run npm-publish -- --release"
  },
  "contributors": [
    "Kirill Volkovich <kirill.volkovich@jcatalog.com>",
    "Alexey Sergeev <alexey.sergeev@jcatalog.com>"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "license": "Apache-2.0",
  "repository": "OpusCapita/react-svg",
  "peerDependencies": {
    "react": "^0.14.8 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.8 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@opuscapita/npm-scripts": "1.0.8",
    "@opuscapita/react-showroom-client": "1.2.8",
    "@opuscapita/react-showroom-server": "1.2.0",
    "@opuscapita/svg-icons": "1.1.0",
    "autoprefixer": "6.4.1",
    "babel-cli": "6.5.1",
    "babel-core": "6.4.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.5.2",
    "babel-standalone": "6.14.0",
    "cross-env": "1.0.7",
    "css-loader": "0.21.0",
    "eslint": "3.11.1",
    "eslint-config-opuscapita": "1.0.7",
    "eslint-plugin-react": "6.10.3",
    "express": "4.13.4",
    "file-loader": "0.9.0",
    "fs-extra": "1.0.0",
    "fuzzysearch": "1.0.3",
    "json-loader": "0.5.4",
    "less": "2.3.1",
    "less-loader": "2.2.2",
    "nodemon": "1.9.1",
    "postcss-loader": "0.13.0",
    "postcss-modules": "0.5.2",
    "prop-types": "15.6.0",
    "raw-loader": "0.5.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "rimraf": "2.5.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "1.5.0",
    "webpack-dev-middleware": "1.10.1",
    "write-file-webpack-plugin": "3.4.2"
  },
  "dependencies": {}
}
